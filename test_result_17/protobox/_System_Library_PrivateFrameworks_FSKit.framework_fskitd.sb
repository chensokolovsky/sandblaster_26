(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "AppleLIFSUserClient"))
		(require-not (extension com.apple.app-sandbox.iokit-client))
		(iokit-user-client-class "AppleKeyStoreUserClient")
		(require-any (iokit-user-client-class-prefix "${ENTITLEMENT:com.apple.security.exception.iokit-user-client-class}")
(iokit-user-client-class-prefix "${ENTITLEMENT:com.apple.security.iokit-user-client-class}"))))
(deny iokit-open-service
	(require-not (iokit-user-client-class "com_apple_filesystems_lifs")))
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-issue-extension)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.logd.events"))
		(require-not (global-name "com.apple.runningboard"))
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.FileCoordination"))
		(require-not (global-name "com.apple.system.libinfo.muser"))
		(require-not (global-name "com.apple.spotlight.userfs"))
		(require-all (require-not (global-name "com.apple.fskit.fskit_helper"))
(require-not (global-name "com.apple.filesystems.smbclientd")))
		(require-not (global-name "com.apple.filesystems.localLiveFiles"))
		(require-not (extension com.apple.app-sandbox.mach))
		(require-not (global-name "com.apple.FileProvider"))
		(require-not (global-name "com.apple.cfprefsd.daemon"))
		(require-not (global-name "com.apple.lsd.mapdb"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.cfprefsd.daemon.system"))
		(require-not (xpc-service-name "com.apple.extensionkitservice"))
		(require-all (require-not (xpc-service-name "com.apple.fskit.msdos"))
(require-not (xpc-service-name "com.apple.fskit.exfat")))
		(require-not (extension com.apple.webkit.extension.mach))
		(require-not (extension com.apple.pluginkit.plugin-service))
		(require-not (global-name "com.apple.DiskArbitration.diskarbitrationd"))
		(require-any
			(global-name "com.apple.dt.testmanagerd.uiprocess")
			(debug-mode))))
(deny process-exec*
	(require-not (literal "/sbin/mount_lifs")))
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 2959 3ba5 1ea8)))
(deny syscall-mach
	(require-not (cb 2963 3ba5 1ea6)))
(deny syscall-mig
	(require-not (cc 2965 3ba5 1ea4)))
(deny system-fcntl
	(require-not (ce 157c 3ba5 1ea2)))
(deny system-fsctl
	(require-not (fsctl-command-regex [UNSUPPORTED](_IO "n" 4))))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 90)
	(mac-policy-name "AMFI")
	(require-all
		(mac-policy-name "Sandbox")
		(require-not (mac-syscall-number 7))
		(require-not (mac-syscall-number 6))
		(mac-syscall-number 4)
		(mac-syscall-number 67)
		(mac-syscall-number 2)))
(deny system-necp-client-action)
(deny storage-class-map)
