(version 1)
(allow default)
(deny file-ioctl
	(require-all
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 113))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 109))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 112))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 108))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 121))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 127))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 106))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 117))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 103))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 102))))
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service
	(require-not (iokit-user-client-class "IOUserNetworkWLAN")))
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-issue-extension)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.logd"))
		(require-not (extension com.apple.pluginkit.plugin-service))
		(require-not (extension com.apple.webkit.extension.mach))
		(require-not (extension com.apple.app-sandbox.mach))
		(global-name "com.apple.SystemConfiguration.configd")
		(require-any
			(global-name "com.apple.dt.testmanagerd.uiprocess")
			(debug-mode))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl
	(require-all
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 56)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "e" 2)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 37)))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 33))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 201))))
(deny syscall-unix
	(require-not (c1 487c 3ba5 01ae)))
(deny syscall-mach
	(require-not (cb 4886 3ba5 01ac)))
(deny syscall-mig
	(require-not (cc 4888 3ba5 01aa)))
(deny system-fcntl
	(require-not (ce 03b5 3ba5 01a8)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
