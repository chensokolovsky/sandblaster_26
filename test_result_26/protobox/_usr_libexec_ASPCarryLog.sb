(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-user-client
	(require-all
		(require-all (require-not (iokit-user-client-class "AppleNVMePassThroughUC"))
(require-not (iokit-user-client-class "IOUSBMassStorageResourceUserClient")))
		(require-not (iokit-user-client-class "AppleNVMeUpdateUC"))
		(iokit-user-client-class "AppleNVMeEANUC")
		(iokit-user-client-class "ASPUserClient")))
(deny iokit-open-service
	(require-all
		(require-not (iokit-user-client-class "AppleNVMeEAN"))
		(require-all (require-not (iokit-user-client-class "AppleNVMePassThrough"))
(require-not (iokit-user-client-class "IOUSBMassStorageResource")))
		(require-not (iokit-user-client-class "AppleEmbeddedNVMeController"))
		(require-not (iokit-user-client-class "AppleANS3NVMeController"))
		(require-not (iokit-user-client-class "AppleANS3CGv2Controller"))
		(iokit-user-client-class "AppleANS2CGNVMeController")
		(iokit-user-client-class "ASPStorage")))
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.logd.events"))
		(require-not (global-name "com.apple.diagnosticpipeline.tasking.service"))
		(require-not (global-name "com.apple.debug.telemetry"))
		(require-not (global-name "com.apple.system.libinfo.muser"))
		(require-not (global-name "com.apple.system.logger"))
		(require-not (global-name "com.apple.diagd"))
		(require-not (global-name "com.apple.mobilegestalt.xpc"))
		(require-not (global-name "com.apple.cfprefsd.daemon"))
		(require-not (global-name "com.apple.mobile.softwareupdated"))
		(require-not (global-name "com.apple.xpc.activity.unmanaged"))
		(require-not (global-name "com.apple.erm.logging"))
		(require-not (global-name "com.apple.cache_delete.public"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.containermanagerd.system"))
		(require-not (global-name "com.apple.cfprefsd.daemon.system"))
		(require-not (global-name "com.apple.cache_delete"))
		(global-name "com.apple.analyticsd")
		(global-name "com.apple.FileCoordination")
		(global-name "com.apple.DiskArbitration.diskarbitrationd")
		(global-name "com.apple.CoreServices.coreservicesd")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 1a7b 9e1d 8f46)))
(deny syscall-mach
	(require-not (cb 1673 9e1d 8f44)))
(deny syscall-mig
	(require-not (cc 1a85 9e1d 8f42)))
(deny system-fcntl
	(require-not (ce 1a8b 9e1d 8f40)))
(deny system-fsctl
	(require-all
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "h" 29)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "h" 22)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 17)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "h" 20)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "h" 28)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "h" 21)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "h" 27)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 18)))))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
