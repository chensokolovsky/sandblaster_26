		(require-any
			(require-all
				(xpc-service-name-regex #".+")
				(require-not (xpc-service-name "com.apple.MTLCompilerService"))
				(require-not (xpc-service-name "com.apple.extensionkitservice"))
				(require-not (extension com.apple.pluginkit.plugin-service)))
			(require-all
				(require-not (global-name "com.apple.DiskArbitration.diskarbitrationd"))
				(require-not (global-name "com.apple.CoreServices.coreservicesd"))
				(require-any
					(xpc-service-name-regex #".+")
					(require-all
						(debug-mode)
						(global-name "com.apple.dt.testmanagerd.uiprocess")))))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 38f7 9e1d 71dc)))
(deny syscall-mach
	(require-not (cb 3901 9e1d 71da)))
(deny syscall-mig
	(require-not (cc 3903 9e1d 71d8)))
(deny system-fcntl
	(require-not (ce 3909 9e1d 71d6)))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 90)
	(mac-policy-name "AMFI")
	(require-all
		(mac-policy-name "Sandbox")
		(require-not (d2 1392 71d3 9e1e))
		(require-not (mac-syscall-number 261))
		(require-not (mac-syscall-number 4))
		(mac-syscall-number 67)
		(mac-syscall-number 2))
	(require-all
		(mac-syscall-number 1)
		(mac-policy-name "vnguard")))
(deny system-necp-client-action)
(deny storage-class-map)
