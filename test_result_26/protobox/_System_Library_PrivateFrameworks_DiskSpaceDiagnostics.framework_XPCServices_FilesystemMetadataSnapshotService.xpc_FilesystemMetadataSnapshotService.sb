(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "AppleNVMeUpdateUC"))
		(iokit-user-client-class "ASPUserClient")))
(deny iokit-open-service
	(require-all
		(iokit-user-client-class "AppleEmbeddedNVMeController")
		(iokit-user-client-class "AppleANS3NVMeController")
		(iokit-user-client-class "AppleANS3CGv2Controller")
		(iokit-user-client-class "AppleANS2CGNVMeController")
		(iokit-user-client-class "ASPStorage")))
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.spaceattributiond"))
		(require-not (global-name "com.apple.system.logger"))
		(require-not (global-name "com.apple.cache_delete"))
		(require-not (global-name "com.apple.cfprefsd.daemon"))
		(require-not (global-name "com.apple.atc.xpc"))
		(require-not (global-name "com.apple.cache_delete.public"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.cfprefsd.daemon.system"))
		(global-name "com.apple.analyticsd")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 521a 9e1d 5b18)))
(deny syscall-mach
	(require-not (cb 0901 9e1d 5b16)))
(deny syscall-mig
	(require-not (cc 5224 9e1d 5b14)))
(deny system-fcntl
	(require-not (ce 522a 9e1d 5b12)))
(deny system-fsctl
	(require-all
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 117)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 131)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 125)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 71)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 37)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 114)))
		(fsctl-command-regex [UNSUPPORTED](_IO "J" 135))
		(fsctl-command-regex [UNSUPPORTED](_IO "J" 113))))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
