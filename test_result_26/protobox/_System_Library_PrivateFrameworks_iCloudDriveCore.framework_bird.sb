(version 1)
(allow default)
(deny file-ioctl)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.aa.daemon.xpc"))
		(require-not (global-name "com.apple.rtcreportingd"))
		(require-not (global-name "com.apple.debug.telemetry"))
		(require-not (global-name "com.apple.iconservices"))
		(require-not (global-name "com.apple.mobile.softwareupdated"))
		(require-not (global-name "com.apple.erm.logging"))
		(require-not (global-name "com.apple.duetactivityscheduler"))
		(require-not (global-name "com.apple.nesessionmanager.content-filter"))
		(require-not (global-name "com.apple.usernotifications.listener"))
		(require-all (require-not (xpc-service-name "com.apple.iCloudDriveCore.telemetry-disk-checker"))
(require-not (xpc-service-name "com.apple.CloudDocsDaemon.container-metadata-extractor")))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 265d 9e1d 8521)))
(deny syscall-mach
	(require-not (cb 1acd 9e1d 851f)))
(deny syscall-mig
	(require-not (cc 2667 9e1d 851d)))
(deny sysctl-write
	(require-not (sysctl-name "kern.rage_vnode")))
(deny system-fcntl
	(require-not (ce 2670 9e1d 851a)))
(deny system-kas-info)
(deny storage-class-map)
