(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 7182 9e1d 3be7)))
(deny syscall-mach
	(require-not (cb 718c 9e1d 3be5)))
(deny syscall-mig
	(require-not (cc 718e 9e1d 3be3)))
(deny sysctl-write
	(require-not (sysctl-name "security.mac.image4.nonce.generate_proposal")))
(deny system-fcntl
	(require-not (ce 719b 9e1d 3be0)))
(deny system-fsctl
	(require-all
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 133)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "A" 25)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "A" 27)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 102)))
		(require-not (fsctl-command-regex [UNSUPPORTED](_IO "A" 26)))))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 101)
	(mac-syscall-number 90)
	(mac-policy-name "AMFI")
	(require-all
		(d2 719e 3bd7 3bd8)
		(require-not (mac-policy-name "Sandbox")))
	(require-all
		(mac-syscall-number 1)
		(mac-policy-name "vnguard")))
(deny storage-class-map)
