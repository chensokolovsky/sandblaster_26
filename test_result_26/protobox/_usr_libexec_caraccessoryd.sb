(deny process-exec*
	(require-not (literal "/usr/bin/log")))
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 2bc6 9e1d 7e12)))
(deny syscall-mach
	(require-not (cb 2bd0 9e1d 7e10)))
(deny syscall-mig
	(require-not (cc 2bd3 9e1d 7e0e)))
(deny system-fcntl
	(require-not (ce 2bd9 9e1d 7e0c)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
