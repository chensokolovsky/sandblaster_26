(version 1)
(allow default)
(deny file-ioctl)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.devicesharingd"))
		(require-not (global-name "com.apple.mobile.keybagd.xpc"))
		(require-not (global-name "com.apple.coremedia.routingcontext.xpc"))
		(require-not (global-name "com.apple.debug.telemetry"))
		(require-not (global-name "com.apple.kvsd"))
		(require-not (global-name "com.apple.pearld"))
		(require-not (global-name "com.apple.sharingd.nsxpc"))
		(require-not (global-name "com.apple.devicesharingd.enrollmentassetservice"))
		(require-not (global-name "com.apple.coremedia.volumecontroller.xpc"))
		(require-not (global-name "com.apple.erm.logging"))
		(require-not (global-name "com.apple.coremedia.routediscoverer.xpc"))
		(global-name "com.apple.SBUserNotification")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 47b1 9e1d 637b)))
(deny syscall-mach
	(require-not (cb 47bb 9e1d 6379)))
(deny syscall-mig
	(require-not (cc 47bd 9e1d 6377)))
(deny system-fcntl
	(require-not (ce 47c3 9e1d 6375)))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 90)
	(mac-policy-name "AMFI")
	(require-all
		(mac-policy-name "Sandbox")
		(require-not (d2 47c6 9d5e 9e1e))
		(mac-syscall-number 67)
		(mac-syscall-number 2)))
(deny system-necp-client-action
	(require-all
		(d3 019b 9adb 9e1e)
		(necp-client-action-regex [UNSUPPORTED])
		(d3 019c 9add 9e1e)
		(necp-client-action-regex [UNSUPPORTED])
		(d3 019d 9adf 9e1e)
		(necp-client-action-regex [UNSUPPORTED])
		(necp-client-action-regex [UNSUPPORTED])
		(necp-client-action-regex [UNSUPPORTED])))
(deny storage-class-map)
