(deny syscall-mach
	(require-not (cb 8c24 9e1d 2017)))
(deny syscall-mig
	(require-not (cc 8c26 9e1d 2015)))
(deny system-fcntl
	(require-not (ce 387f 9e1d 2013)))
(deny system-kas-info)
(deny storage-class-map)
