(version 1)
(allow default)
(deny file-ioctl
	(require-all
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 113))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 109))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 112))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 108))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 121))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 127))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 106))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 117))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 102))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 103))))
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service
	(require-not (iokit-user-client-class "IOUserNetworkWLAN")))
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.logd"))
		(global-name "com.apple.SystemConfiguration.configd")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl
	(require-all
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "e" 2)))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 17))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 56))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 159))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 37))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 201))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 33))))
(deny syscall-unix
	(require-not (c1 aae0 9e1d 0406)))
(deny syscall-mach
	(require-not (cb aaea 9e1d 0404)))
(deny syscall-mig
	(require-not (cc aaec 9e1d 0402)))
(deny system-fcntl
	(require-not (ce 5730 9e1d 0400)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
