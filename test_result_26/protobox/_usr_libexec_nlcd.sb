(version 1)
(allow default)
(deny file-ioctl)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl
	(require-all
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 145)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 159)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 216)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 146)))))
(deny syscall-unix
	(require-not (c1 783b 9e1d 3270)))
(deny syscall-mach
	(require-not (cb 7845 9e1d 326e)))
(deny syscall-mig
	(require-not (cc 7847 9e1d 326c)))
(deny sysctl-write
	(require-not (sysctl-name "net.link.generic.system.enable_netagent")))
(deny system-fcntl
	(require-not (ce 0378 9e1d 3269)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
