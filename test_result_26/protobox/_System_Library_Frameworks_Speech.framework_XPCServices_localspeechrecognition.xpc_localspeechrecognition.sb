(deny syscall-mach
	(require-not (cb 65d0 9e1d 471e)))
(deny syscall-mig
	(require-not (cc 65d3 9e1d 471c)))
(deny system-fcntl
	(require-not (ce 65d9 9e1d 471a)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
