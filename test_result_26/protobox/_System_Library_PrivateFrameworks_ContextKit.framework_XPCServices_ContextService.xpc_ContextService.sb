(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 4035 9e1d 6b16)))
(deny syscall-mach
	(require-not (cb 403f 9e1d 6b14)))
(deny syscall-mig
	(require-not (cc 4041 9e1d 6b12)))
(deny system-fcntl
	(require-not (ce 4047 9e1d 6b10)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
