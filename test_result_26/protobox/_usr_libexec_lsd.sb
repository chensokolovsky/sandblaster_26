(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 6716 9e1d 457d)))
(deny syscall-mach
	(require-not (cb 27e2 9e1d 457b)))
(deny syscall-mig
	(require-not (cc 6720 9e1d 4579)))
(deny system-fcntl
	(require-not (ce 6726 9e1d 4577)))
(deny system-fsctl
	(require-not (fsctl-command-regex [UNSUPPORTED](_IO "A" 2))))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 98)
	(mac-policy-name "AMFI")
	(mac-syscall-number 96)
	(mac-syscall-number 95)
	(mac-syscall-number 90)
	(require-all
		(mac-policy-name "Sandbox")
		(require-not (d2 6729 4573 9e1e))
		(require-not (mac-syscall-number 2)))
	(require-all
		(mac-syscall-number 1)
		(mac-policy-name "vnguard")))
(deny system-necp-client-action)
(deny storage-class-map)
