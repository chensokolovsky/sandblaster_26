(deny syscall-mach
	(require-not (cb 1acd 9e1d 1d93)))
(deny syscall-mig
	(require-not (cc 8f73 9e1d 1d91)))
(deny system-fcntl
	(require-not (ce 8f79 9e1d 1d8f)))
(deny system-kas-info)
(deny storage-class-map)
