(version 1)
(allow default)
(deny file-ioctl)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.iokit.powerdxpc"))
		(require-not (global-name "com.apple.geod"))
		(require-not (global-name "com.apple.TapToRadarKit.service"))
		(global-name "com.apple.PowerManagement.control")
		(global-name "com.apple.FileCoordination")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 a5a9 9e1d 08a7)))
(deny syscall-mach
	(require-not (cb 4e20 9e1d 08a5)))
(deny syscall-mig
	(require-not (cc 6373 9e1d 08a3)))
(deny system-fcntl
	(require-not (ce a5b3 9e1d 08a1)))
(deny system-kas-info)
(deny storage-class-map)
