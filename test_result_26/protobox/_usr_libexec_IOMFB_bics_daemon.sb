(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "AppleNVMeEANUC"))
		(require-not (iokit-user-client-class "AppleEmbeddedTouchEEPROMDriverUC"))
		(require-not (iokit-user-client-class "RootDomainUserClient"))
		(iokit-user-client-class "AFKEndpointInterfaceUserClient")))
(deny iokit-open-service
	(require-all
		(require-not (iokit-user-client-class "AppleNVMeEAN"))
		(require-not (iokit-user-client-class "AppleEmbeddedTouchEEPROMDriver"))
		(require-not (iokit-user-client-class "IOMobileFramebufferShim"))
		(require-not (iokit-user-client-class "IOMobileFramebuffer"))
		(require-not (iokit-user-client-class "IOPMrootDomain"))
		(iokit-user-client-class "AFKEPInterfaceKextV2")))
(deny iokit-set-properties
	(require-not (iokit-property "IOMFBProxOcclusionLevel"))
	(require-all
		(iokit-user-client-class "AppleANS2CGNVMeController")
		(require-all (require-not (iokit-property "fw-write-done"))
(require-not (iokit-property "bis-write-done")))))
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create
	(require-not (ipc-posix-name "iomfb_bics_daemon.started")))
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.system.logger"))
		(require-not (global-name "com.apple.diagd"))
		(require-not (global-name "com.apple.iohideventsystem"))
		(require-not (global-name "com.apple.logd"))
		(global-name "com.apple.cfprefsd.daemon")
		(global-name "com.apple.cfprefsd.daemon.system")
		(global-name "com.apple.analyticsd")
		(global-name "com.apple.CARenderServer")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 62c5 9e1d 4adf)))
(deny syscall-mach
	(require-not (cb 62cf 9e1d 4add)))
(deny syscall-mig
	(require-not (cc 62d1 9e1d 4adb)))
(deny system-fcntl
	(require-not (ce 0cc4 9e1d 4ad9)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
