(deny syscall-mach
	(require-not (cb 53bb 9e1d 59aa)))
(deny syscall-mig
	(require-not (cc 53bd 9e1d 59a8)))
(deny system-fcntl
	(require-not (ce 53c3 9e1d 59a6)))
(deny system-kas-info)
(deny storage-class-map)
