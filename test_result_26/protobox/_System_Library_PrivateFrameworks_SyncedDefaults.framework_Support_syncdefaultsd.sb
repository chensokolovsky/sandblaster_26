(deny syscall-mach
	(require-not (cb a049 9e1d 0d70)))
(deny syscall-mig
	(require-not (cc a04b 9e1d 0d6e)))
(deny system-fcntl
	(require-not (ce 2fbc 9e1d 0d6c)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
