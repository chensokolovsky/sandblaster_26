		(require-not (global-name "com.apple.CoreServices.coreservicesd"))
		(require-not (global-name "com.apple.AppSSO.service-xpc"))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 09d5 9e1d 9aea)))
(deny syscall-mach
	(require-not (cb 09df 9e1d 9ae8)))
(deny syscall-mig
	(require-not (cc 09e1 9e1d 9ae6)))
(deny system-fcntl
	(require-not (ce 09e7 9e1d 9ae4)))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 90)
	(mac-policy-name "AMFI")
	(require-all
		(mac-policy-name "Sandbox")
		(require-not (d2 09ea 9ae2 9e1e))
		(require-not (d2 09eb 9b59 9e1e))
		(mac-syscall-number 67)
		(mac-syscall-number 2))
	(require-all
		(mac-syscall-number 1)
		(mac-policy-name "vnguard")))
(deny system-necp-client-action
	(require-all
		(require-not (necp-client-action-regex [UNSUPPORTED]))
		(require-not (d3 019b 9adb 9e1e))
		(require-not (necp-client-action-regex [UNSUPPORTED]))
		(require-not (d3 019c 9add 9e1e))
		(require-not (necp-client-action-regex [UNSUPPORTED]))
		(require-not (d3 019d 9adf 9e1e))
		(require-not (necp-client-action-regex [UNSUPPORTED]))
		(necp-client-action-regex [UNSUPPORTED])
		(necp-client-action-regex [UNSUPPORTED])))
(deny storage-class-map)
