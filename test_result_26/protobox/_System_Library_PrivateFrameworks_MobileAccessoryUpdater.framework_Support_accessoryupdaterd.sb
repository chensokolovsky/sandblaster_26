		(require-all (require-not (xpc-service-name "com.apple.UARPUpdaterServiceAFU"))
(require-not (xpc-service-name "com.apple.UARPUpdaterServiceDFU"))
(require-not (xpc-service-name "com.apple.UARPUpdaterServiceHID"))
(require-not (xpc-service-name "com.apple.UARPUpdaterServiceUSBPD"))
(require-not (xpc-service-name "com.apple.UARPUpdaterServiceLegacyAudio"))
(require-not (xpc-service-name "com.apple.MobileAccessoryUpdater.EAUpdaterService"))
(require-not (xpc-service-name "com.apple.MobileAccessoryUpdater.DurianUpdaterService"))
(require-not (xpc-service-name "com.apple.MobileAccessoryUpdater.StandaloneHIDAudService"))
