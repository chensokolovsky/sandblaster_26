(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 2b05 9e1d 7f49)))
(deny syscall-mach
	(require-not (cb 2b0f 9e1d 7f47)))
(deny syscall-mig
	(require-not (cc 2b11 9e1d 7f45)))
(deny system-fcntl
	(require-not (ce 2b17 9e1d 7f43)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
