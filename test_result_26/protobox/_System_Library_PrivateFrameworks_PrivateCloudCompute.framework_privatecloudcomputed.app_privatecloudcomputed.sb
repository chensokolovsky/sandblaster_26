(deny syscall-mach
	(require-not (cb 8596 9e1d 282e)))
(deny syscall-mig
	(require-not (cc 8598 9e1d 282c)))
(deny system-fcntl
	(require-not (ce 859e 9e1d 282a)))
(deny system-kas-info)
(deny storage-class-map)
