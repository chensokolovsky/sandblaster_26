(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.logd.events"))
		(require-not (global-name "com.apple.runningboard"))
		(require-not (global-name "com.apple.research.adtcd"))
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.mobile.keybagd.UserManager.xpc"))
		(require-not (global-name "com.apple.system.libinfo.muser"))
		(require-not (global-name "com.apple.mobile.usermanagerd.xpc"))
		(require-not (global-name "com.apple.biome.access.user"))
		(require-not (global-name "com.apple.cfprefsd.daemon"))
		(require-not (global-name "com.apple.lsd.mapdb"))
		(require-not (global-name "com.apple.distributed_notifications@1v3"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.containermanagerd.system"))
		(require-not (global-name "com.apple.cfprefsd.daemon.system"))
		(require-all (require-not (xpc-service-name "com.apple.siri.GMSSELFIngestor"))
(require-not (xpc-service-name "com.apple.siri.LLMCacheSELFIngestor"))
(require-not (xpc-service-name "com.apple.NanoOxygenSaturation.watchkitapp.NanoOxygenSaturationWidgetExtension"))
(require-not (xpc-service-name "com.apple.lighthouse.BiomeSELFIngestor"))
(require-not (xpc-service-name "com.apple.lighthouse.TranscriptSELFMapper"))
(require-not (xpc-service-name "com.apple.lighthouse.IFTelemetrySELFIngestor"))
(require-not (xpc-service-name "com.apple.lighthouse.IFTranscriptSELFIngestor"))
(require-not (xpc-service-name "com.apple.lighthouse.IFResponseGenerationSELFIngestor")))
		(require-any
			(require-all
				(xpc-service-name-regex #".+")
				(require-not (xpc-service-name "com.apple.extensionkitservice"))
				(require-not (extension com.apple.pluginkit.plugin-service)))
			(require-all
				(global-name "com.apple.ERFoundationExtensionDaemon")
				(global-name "com.apple.analyticsd")
				(global-name "com.apple.CoreServices.coreservicesd")))))
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 8b54 9e1d 215f)))
(deny syscall-mach
	(require-not (cb 8b5e 9e1d 215d)))
(deny syscall-mig
	(require-not (cc 8b60 9e1d 215b)))
(deny system-fcntl
	(require-not (ce 8b66 9e1d 2159)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
