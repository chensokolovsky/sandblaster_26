(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny process-exec*
	(require-not (literal "/System/Library/Filesystems/apfs.fs/newfs_apfs")))
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 9db8 9e1d 1083)))
(deny syscall-mach
	(require-not (cb 9dc2 9e1d 1081)))
(deny syscall-mig
	(require-not (cc 9dc4 9e1d 107f)))
(deny system-fcntl
	(require-not (ce 9dca 9e1d 107d)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
