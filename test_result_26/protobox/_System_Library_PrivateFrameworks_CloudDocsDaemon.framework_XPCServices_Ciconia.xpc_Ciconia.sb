(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.logd"))))
(deny process-exec*
	(require-all
		(literal "/usr/local/bin/dastool")
		(debug-mode))
	(require-all
		(require-not (literal "/usr/bin/awk"))
		(require-not (literal "/usr/bin/fileproviderctl"))
		(require-not (literal "/usr/bin/env"))))
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 2e46 9e1d 7a9b)))
(deny syscall-mach
	(require-not (cb 2e50 9e1d 7a99)))
(deny syscall-mig
	(require-not (cc 2e52 9e1d 7a97)))
(deny system-fcntl
	(require-not (ce 2e58 9e1d 7a95)))
(deny system-fsctl
	(require-all
		(fsctl-command-regex [UNSUPPORTED](_IO "J" 2))
		(fsctl-command-regex [UNSUPPORTED](_IO "J" 32))))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
