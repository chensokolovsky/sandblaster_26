(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "IOUserUserClient"))
		(require-not (iokit-user-client-class "IOMobileFramebufferUserClient"))
		(require-not (iokit-user-client-class "AppleBCMWLANUserClient"))))
(deny iokit-open-service
	(require-all
		(require-not (iokit-user-client-class "AppleBCMWLANCore"))
		(require-not (iokit-user-client-class "IOUserService"))))
(deny iokit-set-properties
	(require-not (iokit-property "PlatformConfig")))
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 acaa 9e1d 01ee)))
(deny syscall-mach
	(require-not (cb acb4 9e1d 01ec)))
(deny syscall-mig
	(require-not (cc acb7 9e1d 01ea)))
(deny system-fcntl
	(require-not (ce 0378 9e1d 01e8)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
