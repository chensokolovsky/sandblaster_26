(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl
	(require-all
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 56)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 142)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 159)))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 33))))
(deny syscall-unix
	(require-not (c1 3c3a 9e1d 6fb1)))
(deny syscall-mach
	(require-not (cb 3c44 9e1d 6faf)))
(deny syscall-mig
	(require-not (cc 3c47 9e1d 6fad)))
(deny system-fcntl
	(require-not (ce 3c4d 9e1d 6fab)))
(deny system-fsctl
	(require-not (fsctl-command-regex [UNSUPPORTED](_IO "J" 128))))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
