(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 2e83 9e1d 7a5b)))
(deny syscall-mach
	(require-not (cb 2e8d 9e1d 7a59)))
(deny syscall-mig
	(require-not (cc 2e8f 9e1d 7a57)))
(deny system-fcntl
	(require-not (ce 0d6d 9e1d 7a55)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
