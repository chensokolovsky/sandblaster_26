(version 1)
(allow default)
(deny file-ioctl
	(require-all
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 76)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 78)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 24)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 22)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 31)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 73)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 77)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "d" 25)))))
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data
	(require-not (ipc-posix-name "com.apple.featureflags.shm")))
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny job-creation)
(deny mach-issue-extension)
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 56b9 9e1d 572f)))
(deny syscall-mach
	(require-not (cb 1133 9e1d 572d)))
(deny syscall-mig
	(require-not (cc 1287 9e1d 572b)))
(deny system-fcntl
	(require-not (ce 0d6d 9e1d 5729)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
