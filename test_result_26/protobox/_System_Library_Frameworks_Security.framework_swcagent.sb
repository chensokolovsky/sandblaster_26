(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 9f38 9e1d 0edd)))
(deny syscall-mach
	(require-not (cb 9f42 9e1d 0edb)))
(deny syscall-mig
	(require-not (cc 9f44 9e1d 0ed9)))
(deny system-fcntl
