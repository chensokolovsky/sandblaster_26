(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 6565 9e1d 47bd)))
(deny syscall-mach
	(require-not (cb 656f 9e1d 47bb)))
(deny syscall-mig
	(require-not (cc 6571 9e1d 47b9)))
(deny system-fcntl
	(require-not (ce 6551 9e1d 47b7)))
(deny system-kas-info)
(deny system-mac-syscall
