(deny syscall-mach
	(require-not (cb 6224 9e1d 4b4c)))
(deny syscall-mig
	(require-not (cc 6226 9e1d 4b4a)))
(deny system-fcntl
	(require-not (ce 622c 9e1d 4b48)))
(deny system-kas-info)
(deny system-mac-syscall
