(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 41e4 9e1d 6880)))
(deny syscall-mach
	(require-not (cb 2555 9e1d 687e)))
(deny syscall-mig
	(require-not (cc 41ee 9e1d 687c)))
(deny system-fcntl
	(require-not (ce 0df5 9e1d 687a)))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 90)
	(mac-policy-name "AMFI")
	(require-all
		(mac-policy-name "Sandbox")
		(require-not (d2 09ea 71d3 9e1e))
		(mac-syscall-number 261)
		(mac-syscall-number 4)
		(mac-syscall-number 67)
		(mac-syscall-number 2))
	(require-all
		(mac-syscall-number 1)
		(mac-policy-name "vnguard")))
(deny storage-class-map)
