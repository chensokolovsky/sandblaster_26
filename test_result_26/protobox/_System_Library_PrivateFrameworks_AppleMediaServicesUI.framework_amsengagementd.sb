(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 11c1 9e1d 9575)))
(deny syscall-mach
	(require-not (cb 11cb 9e1d 9573)))
(deny syscall-mig
	(require-not (cc 11cd 9e1d 9571)))
(deny system-fcntl
	(require-not (ce 11d3 9e1d 956f)))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 90)
	(mac-policy-name "AMFI")
	(require-all
		(mac-policy-name "Sandbox")
		(require-not (d2 11d6 9d84 9e1e))
		(mac-syscall-number 2))
	(require-all
		(mac-syscall-number 1)
		(mac-policy-name "vnguard")))
(deny system-memorystatus-control
	(require-not (memorystatus-control-command MEMORYSTATUS_CMD_GET_MEMLIMIT_PROPERTIES)))
(deny storage-class-map)
