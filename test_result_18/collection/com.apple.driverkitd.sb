(version 1)
(allow default)
(deny file-read*
	(require-all
		(require-not (subpath "/System/Library/DriverExtensions"))
		(require-not (literal "/dev/console"))
		(require-all (require-not (subpath "/private/var/db/driverkitd"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.driverkitd")))
		(require-not (subpath "/Applications"))
		(require-not (subpath "/private/var/containers/Bundle/Application"))))
(deny file-write*
	(require-all
		(require-not (literal "/dev/console"))
		(require-any (subpath "/private/var/db/driverkitd")
(subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.driverkitd"))))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny iokit-get-properties
	(require-any (iokit-property "OSKernelCPUType")
(iokit-property "OSKernelCPUSubtype"))
	(require-all
		(iokit-property "supports-third-party-drivers")
		(iokit-user-client-class "IOPlatformDevice")))
(deny syscall-unix
	(require-not (c1 467a 6ffb 6368)))
(deny syscall-mach
	(require-not (cb 4684 6fe8 6366)))
(deny syscall-mig
	(require-not (cc 4687 6363 6364)))
(deny sysctl-read
	(require-all
		(require-not (sysctl-name "kern.bootargs"))
		(sysctl-name "kern.osbuildconfig")))
(deny system-kas-info)
(deny storage-class-map)
