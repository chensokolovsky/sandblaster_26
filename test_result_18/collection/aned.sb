(version 1)
(allow default)
(deny file-read*
	(require-all
		(vnode-type REGULAR-FILE)
		(subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.aned"))
	(require-all
		(vnode-type DIRECTORY)
		(subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.aned"))
	(require-all
		(subpath-prefix "${HOME}/Library/Caches/com.apple.aned")
		(extension com.apple.aned.read-only)))
(deny file-read-metadata
	(require-all
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.aned"))
		(require-not (literal-prefix "${HOME}/Library/Caches"))))
(deny file-write*
	(subpath-prefix "${HOME}/Library/Caches/com.apple.aned")
	(require-all
		(vnode-type REGULAR-FILE)
		(subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.aned"))
	(require-all
		(vnode-type DIRECTORY)
		(subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.aned")))
(deny file-write-create
	(require-all
		(vnode-type DIRECTORY)
		(require-not (literal-prefix "${HOME}/Library/Caches"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.aned")
		(subpath-prefix "${HOME}/Library/Caches/com.apple.aned")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny iokit-get-properties
	(require-all
		(require-any (iokit-user-client-class "AppleARMIODevice")
(iokit-user-client-class "IOPlatformExpertDevice"))
		(require-not (iokit-property "ane-type")))
	(require-all
		(require-not (iokit-user-client-class "AppleVirtIONeuralEngineDevice"))
		(iokit-user-client-class "H11ANEIn")))
(deny iokit-open-user-client
	(require-all (require-not (iokit-user-client-class "H11ANEInUserClient"))
(require-not (iokit-user-client-class "AppleVirtIONeuralEngineDeviceIndirectUserClient"))))
(deny job-creation)
(deny system-kas-info)
(deny storage-class-map)
