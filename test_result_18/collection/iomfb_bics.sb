(version 1)
(allow default)
(deny file-read*
	(require-all
		(require-not (subpath "/private/var/hardware"))
		(require-all (require-not (subpath "/private/var/displaycompensationd"))
(require-not (subpath "/private/var/iomfb_bics_daemon"))
(require-not (literal "/dev/disk2"))
(require-not (literal "/dev/disk3"))
(require-not (literal "/dev/disk1")))
		(require-any
			(require-all
				(debug-mode)
				(subpath "/AppleInternal")
				(subpath "/usr/local/lib"))
			(require-all
				(require-any (subpath "/private/var/hardware/FactoryData")
(literal "/usr/standalone/firmware/apticket.der"))
				(subpath "/private/preboot")))))
(deny file-read-metadata
	(require-all
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))))
(deny file-write*
	(require-all
		(require-not (subpath "/private/var/hardware"))
		(require-any (subpath "/private/var/displaycompensationd")
(subpath "/private/var/iomfb_bics_daemon")
(literal "/dev/disk2")
(literal "/dev/disk3")
(literal "/dev/disk1"))))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny iokit-open-user-client
	(require-all
		(require-all (require-not (iokit-user-client-class "AppleNVMeEANUC"))
(require-not (iokit-user-client-class "AppleEmbeddedTouchEEPROMDriverUC")))
		(require-not (iokit-user-client-class "AppleSEPUserClient"))))
(deny iokit-set-properties
	(require-all
		(require-not (iokit-user-client-class "AppleNVMeController"))
		(iokit-user-client-class "AppleSEPUserClient")))
(deny ipc-posix-sem*
	(require-any (ipc-posix-name "iomfb_bics_daemon.started")
(ipc-posix-name "displaycompensationd.started")))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.CARenderServer"))
		(global-name "com.apple.iokit.powerdxpc")
		(global-name "com.apple.PowerManagement.control")
		(extension com.apple.security.exception.mach-lookup.global-name)
		(extension com.apple.security.exception.mach-lookup.local-name)
		(global-name "com.apple.powerlog.plxpclogger.xpc")
		(require-any
			(require-all
				(global-name "com.apple.system.notification_center")
				(entitlement-is-bool-true com.apple.security.on-demand-install-capable)
				(require-any
					(xpc-service-name-regex #".+")
					(xpc-service-name "com.apple.WebKit.")
					(xpc-service-name "com.apple.CoreGraphics.CGPDFService")))
			(require-all
				(global-name "com.apple.lsd.openurl")
				(global-name "com.apple.tccd")
				(global-name "com.apple.lsd.advertisingidentifiers")
				(xpc-service-name-regex #".+")))))
(deny system-kas-info)
(deny storage-class-map)
