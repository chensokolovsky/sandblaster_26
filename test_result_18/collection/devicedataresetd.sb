(version 1)
(allow default)
(deny file-write-setugid)
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.CARenderServer"))
		(require-not (global-name "com.apple.mobile.obliteration"))
		(require-not (global-name "com.apple.nfcd.hwmanager"))))
(deny system-kas-info)
(deny user-preference-read
	(require-all
		(require-not (preference-domain "com.apple.coreanimation"))
		(require-not (preference-domain "com.apple.ImageIO"))
		(require-any (preference-domain "com.apple.devicedataresetd")
(preference-domain "com.apple.devicedatareset.devicedataresetd"))))
(deny user-preference-write
	(require-any (preference-domain "com.apple.devicedataresetd")
(preference-domain "com.apple.devicedatareset.devicedataresetd")))
(deny storage-class-map)
