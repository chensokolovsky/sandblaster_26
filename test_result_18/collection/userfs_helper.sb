(version 1)
(allow default)
(deny file-ioctl
	(require-any (regex #"/dev/disk[0-9]")
(regex #"/dev/rdisk[0-9]")))
(deny file-issue-extension
	(require-all
		(subpath-prefix "${HOME}/Library/Caches/com.apple.userfs_helper")
		(extension-class "com.apple.app-sandbox.read-write")
		(extension-class "com.apple.app-sandbox.read")
		(extension-class "com.apple.mediaserverd.read-write")
		(extension-class "com.apple.aned.read-only")
		(extension-class "com.apple.mediaserverd.read")))
(deny file-read*
	(require-all
		(vnode-type REGULAR-FILE)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.userfs_helper")))
	(require-all
		(vnode-type DIRECTORY)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.userfs_helper")))
	(require-all
		(require-all (require-not (regex #"/dev/disk[0-9]"))
(require-not (regex #"/dev/rdisk[0-9]")))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.userfs_helper"))))
(deny file-read-metadata
	(require-all
		(require-not (literal-prefix "${HOME}/Library/Caches"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.userfs_helper"))))
(deny file-write*
	(subpath-prefix "${HOME}/Library/Caches/com.apple.userfs_helper")
	(require-all
		(vnode-type REGULAR-FILE)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.userfs_helper")))
	(require-all
		(vnode-type DIRECTORY)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.userfs_helper"))))
(deny file-write-create
	(require-all
		(vnode-type DIRECTORY)
		(require-not (literal-prefix "${HOME}/Library/Caches"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.userfs_helper")
		(subpath-prefix "${HOME}/Library/Caches/com.apple.userfs_helper")))
(deny file-write-data
	(require-all (require-not (regex #"/dev/disk[0-9]"))
(require-not (regex #"/dev/rdisk[0-9]"))))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny system-kas-info)
(deny storage-class-map)
