(version 1)
(allow default)
(deny file-issue-extension
	(require-all
		(extension-class "com.apple.app-sandbox.read-write")
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted")))
	(require-all
		(extension-class "com.apple.app-sandbox.read")
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted")))
	(require-all
		(extension-class "com.apple.mediaserverd.read-write")
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted")))
	(require-all
		(extension-class "com.apple.aned.read-only")
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted")))
	(require-all
		(extension-class "com.apple.mediaserverd.read")
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted")))
	(require-all
		(extension-class "com.apple.Pasteboard-readonly")
		(require-not (literal-prefix "${FRONT_USER_HOME}/Library/Caches/com.apple.Pasteboard/"))))
(deny file-read*
	(require-all
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.Pasteboard"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.UIKit.pboard"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted"))
		(require-any
			(require-all
				(debug-mode)
				(subpath "/AppleInternal")
				(subpath "/usr/local/lib"))
			(require-all
				(extension com.apple.app-sandbox.read)
				(extension com.apple.app-sandbox.read-write)
				(extension com.apple.security.exception.files.absolute-path.read-only)
				(extension com.apple.security.exception.files.absolute-path.read-write)
				(require-any
					(require-any (subpath "/private/var/hardware/FactoryData")
(literal "/usr/standalone/firmware/apticket.der"))
					(require-any (literal "/System/Library/Caches/apticket.der")
(subpath "/System/Library/Caches/com.apple.factorydata")
(subpath "/System/Library/Caches/com.apple.kernelcaches")))))))
(deny file-read-metadata
	(require-all
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.Pasteboard")
		(subpath-prefix "${HOME}/Library/Caches/com.apple.UIKit.pboard")
		(subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted")
		(require-any
			(require-all
				(debug-mode)
				(subpath "/AppleInternal")
				(subpath "/usr/local/lib"))
			(require-all
				(extension com.apple.app-sandbox.read)
				(extension com.apple.app-sandbox.read-write)
				(extension com.apple.security.exception.files.absolute-path.read-only)
				(extension com.apple.security.exception.files.absolute-path.read-write)
				(require-any
					(require-any (subpath "/private/var/hardware/FactoryData")
(literal "/usr/standalone/firmware/apticket.der"))
					(require-any (literal "/System/Library/Caches/apticket.der")
(subpath "/System/Library/Caches/com.apple.factorydata")
(subpath "/System/Library/Caches/com.apple.kernelcaches")))))))
(deny file-write*
	(require-all
		(require-not (subpath "/private/var/tmp/com.apple.pasteboard.pasted"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.Pasteboard"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.UIKit.pboard"))
		(subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.pasteboard.pasted")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.FileCoordination"))
		(require-not (global-name "com.apple.CARenderServer"))
		(require-not (global-name "com.apple.fontservicesd"))
		(require-not (global-name "com.apple.backboard.hid.services"))
		(require-not (global-name "com.apple.FileProvider"))
		(require-all (require-not (global-name "com.apple.DragUI.druid.paste-announcement"))
(require-not (global-name "com.apple.coreservices.uasharedpasteboardmanager.xpc")))
		(require-not (global-name "com.apple.audio.SystemSoundServer-iOS"))
		(require-not (global-name "com.apple.backboard.hid-services.xpc"))))
(deny process-exec*
	(literal "/System/Library/Frameworks/UIKit.framework/Support/pasteboardd"))
(deny system-kas-info)
(deny user-preference-read
	(preference-domain "com.apple.iclouddrive.features"))
(deny storage-class-map)
