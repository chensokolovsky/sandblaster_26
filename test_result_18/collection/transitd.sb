(version 1)
(allow default)
(deny file-read*
	(require-not (subpath-prefix "${HOME}/Library/Application Support/com.apple.mdt")))
(deny file-write*
	(require-all
		(require-all (require-not (subpath "/private/var/spool/mdt"))
(require-not (regex #"/private/var/PersonaVolumes/[UNSUPPORTED]/Containers/Data/[UNSUPPORTED]/[UNSUPPORTED]/Documents/Inbox"))
(require-not (subpath-prefix "${HOME}/Library/Inboxes"))
(require-not (subpath-prefix "${HOME}/Library/Application Support/Containers"))
(require-not (subpath-prefix "${HOME}/Library/Application Support/com.apple.mdt")))
		(require-not (literal-prefix "${HOME}/Library/Application Support"))
		(literal "/private/var/")
		(regex #"^/private/var/mobile/Containers/Data/[^/]+/[^/]+/Documents/Inbox" #"^/private/var/mobile/Containers/Temp/Data/[^/]+/[^/]+/Documents/Inbox" #"^/private/var/euser[0-9]+/Containers/(Temp/)?Data/[^/]+/[^/]+/Documents/Inbox" #"^/private/var/[-0-9A-F]+/Containers/(Temp/)?Data/[^/]+/[^/]+/Documents/Inbox" #"^/private/var/Users/[^/]+/Containers/(Temp/)?Data/[^/]+/[^/]+/Documents/Inbox")
		(subpath-prefix "${FRONT_USER_HOME}")
		(subpath-prefix "${HOME}")))
(deny file-write-setugid)
(deny file-write-unlink
	(require-all
		(require-not (subpath-prefix "${HOME}/Downloads"))
		(require-any
			(entitlement-is-present com.apple.private.oop-jit.loader)
			(require-all
				(extension com.apple.sandbox.oopjit)
				(require-not (subpath "/private/var/OOPJit"))))))
(deny job-creation)
(deny system-kas-info)
(deny storage-class-map)
