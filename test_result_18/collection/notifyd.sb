(version 1)
(allow default)
(deny file-read*
	(require-all
		(require-not (literal "/private"))
		(require-not (literal "/private/var"))
		(require-all (require-not (literal "/Library"))
(require-not (literal "/private/var/db"))
(require-not (literal "/private/var/networkd"))
(require-not (literal "/private/var/tmp"))
(require-not (literal "/private/var/tmp/notify_pathwatch.test"))
(require-not (literal "/private/var/preferences"))
(require-not (literal "/private/var/preferences/SystemConfiguration"))
(require-not (literal-prefix "${FRONT_USER_HOME}/tmp"))
(require-not (literal-prefix "${FRONT_USER_HOME}/tmp/notify_pathwatch.test")))
		(require-all (require-not (subpath "/private/var/tmp/Bom.dst"))
(require-not (literal "/private/var/run/notifyd_"))
(require-not (literal "/private/var/log/notifyd.log"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/tmp/Bom.dst")))
		(require-not (subpath "/private/var/preferences/FeatureFlags"))
		(require-not (literal "/tmp"))
		(require-not (subpath "/usr"))
		(require-all (require-not (literal "/etc"))
(require-not (literal "/var")))
		(require-not (subpath "/private/etc"))
		(require-any
			(extension com.apple.app-sandbox.read)
			(extension com.apple.app-sandbox.read-write)
			(extension com.apple.security.exception.files.absolute-path.read-only)
			(extension com.apple.security.exception.files.absolute-path.read-write)
			(require-any (subpath "/private/var/hardware/FactoryData")
(literal "/usr/standalone/firmware/apticket.der"))
			(require-any (literal "/System/Library/Caches/apticket.der")
(subpath "/System/Library/Caches/com.apple.factorydata")
(subpath "/System/Library/Caches/com.apple.kernelcaches"))
			(require-all
				(subpath "/AppleInternal")
				(debug-mode)))))
(deny file-write*
	(require-any (subpath "/private/var/tmp/Bom.dst")
(literal "/private/var/run/notifyd_")
(literal "/private/var/log/notifyd.log")
(subpath-prefix "${FRONT_USER_HOME}/tmp/Bom.dst")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny syscall-unix
	(c1 6fdb 2cf5 9ce0))
(deny sysctl-read
	(require-all
		(require-not (sysctl-name "hw.ephemeral_storage"))
		(sysctl-name "kern.osbuildconfig")))
(deny system-kas-info)
(deny storage-class-map)
