(version 1)
(allow default)
(deny file-read*
	(require-all
		(require-not (subpath "/private/var/tmp/powerlog"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/powerlog"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/CrashReporter/BatteryLife"))))
(deny file-write*
	(require-all
		(require-not (subpath "/private/var/tmp/powerlog"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/powerlog"))
		(subpath-prefix "${FRONT_USER_HOME}/Library/Logs/CrashReporter/BatteryLife")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny system-kas-info)
(deny storage-class-map)
