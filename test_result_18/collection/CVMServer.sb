(version 1)
(allow default)
(deny file-read*
	(require-all
		(require-all (require-not (subpath "/usr/bin/codesign_allocate"))
(require-not (subpath "/System/Library/Frameworks/OpenGLES.framework")))
		(require-not (extension com.apple.app-sandbox.read-write))))
(deny file-write*
	(extension com.apple.app-sandbox.read-write))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.cvmsCompAgent"))
		(require-not (global-name "com.apple.securityd"))
		(require-not (extension com.apple.security.exception.mach-lookup.global-name))
		(require-not (extension com.apple.security.exception.mach-lookup.local-name))
		(require-not (global-name "com.apple.trustd"))
		(require-any
			(require-all
				(global-name "com.apple.system.notification_center")
				(entitlement-is-bool-true com.apple.security.on-demand-install-capable)
				(require-any
					(xpc-service-name-regex #".+")
					(xpc-service-name "com.apple.WebKit.")
					(xpc-service-name "com.apple.CoreGraphics.CGPDFService")))
			(require-all
				(global-name "com.apple.lsd.openurl")
				(global-name "com.apple.tccd")
				(global-name "com.apple.lsd.advertisingidentifiers")
				(xpc-service-name-regex #".+")))))
(deny process-exec*
	(require-any (subpath "/usr/bin/codesign_allocate")
(subpath "/System/Library/Frameworks/OpenGLES.framework")))
(deny system-kas-info)
(deny storage-class-map)
