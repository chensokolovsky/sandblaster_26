(version 1)
(allow default)
(deny file-issue-extension
	(require-all
		(require-not (extension com.apple.afc.root))
		(require-any
			(require-all
				(subpath-prefix "${HOME}/Library/Caches/com.apple.afc")
				(require-not (extension-class "com.apple.mediaserverd.read-write"))
				(require-not (extension-class "com.apple.mediaserverd.read")))
			(require-all
				(extension-class "com.apple.app-sandbox.read-write")
				(subpath-prefix "${HOME}/Library/Caches/com.apple.afc"))
			(require-all
				(extension-class "com.apple.app-sandbox.read")
				(subpath-prefix "${HOME}/Library/Caches/com.apple.afc"))
			(require-all
				(extension-class "com.apple.mediaserverd.read-write")
				(subpath-prefix "${HOME}/Library/Caches/com.apple.afc"))
			(require-all
				(extension-class "com.apple.aned.read-only")
				(subpath-prefix "${HOME}/Library/Caches/com.apple.afc"))
			(require-all
				(extension-class "com.apple.mediaserverd.read")
				(subpath-prefix "${HOME}/Library/Caches/com.apple.afc")))))
(deny file-read*
	(require-all
		(vnode-type REGULAR-FILE)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.afc")))
	(require-all
		(vnode-type DIRECTORY)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.afc")))
	(require-all
		(require-not (literal "/private/var/preferences/com.apple.security.plist"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.afc"))
		(require-all (require-not (literal "/AFCDEBUG"))
(require-not (literal "/Library/Preferences/SystemConfiguration/com.apple.afc.DeviceInfo.plist")))
		(require-not (extension com.apple.afc.root))
		(require-any
			(require-all
				(debug-mode)
				(require-not (subpath "/AppleInternal"))
				(require-not (subpath "/usr/local/lib")))
			(require-all
				(require-all (require-not (subpath "/private/var/hardware/FactoryData"))
(require-not (literal "/usr/standalone/firmware/apticket.der")))
				(require-not (subpath "/private/preboot"))))))
(deny file-read-metadata
	(require-all
		(require-not (literal "/private"))
		(require-not (literal "/Library/Preferences/SystemConfiguration"))
		(require-not (literal "/private/var"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.afc"))
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Caches"))
		(require-not (literal-prefix "${HOME}/Media/Photos"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))
		(require-not (literal "/Library/Preferences"))))
(deny file-write*
	(require-all
		(require-all (require-not (subpath-prefix "${HOME}/Media/Photos/Sync"))
(require-not (subpath-prefix "${HOME}/Media/Photos/Thumbs")))
		(require-any
			(require-all
				(subpath-prefix "${HOME}/Media/MediaAnalysis")
				(extension com.apple.afc.root))
			(require-all
				(subpath-prefix "${HOME}/Media/PhotoData")
				(extension com.apple.afc.root))
			(require-all
				(subpath-prefix "${HOME}/Media/PhotoStreamsData")
				(extension com.apple.afc.root))
			(require-all
				(subpath-prefix "${HOME}/Media/DCIM")
				(extension com.apple.afc.root))
			(require-all
				(subpath-prefix "${HOME}/Media/Photos")
				(extension com.apple.afc.root))
			(require-all
				(vnode-type REGULAR-FILE)
				(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.afc")))
			(require-all
				(vnode-type DIRECTORY)
				(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.afc")))
			(require-all
				(require-not (extension com.apple.afc.root))
				(subpath-prefix "${HOME}/Library/Caches/com.apple.afc")))))
(deny file-write-create
	(require-all
		(vnode-type DIRECTORY)
		(require-not (literal-prefix "${HOME}/Library/Caches"))
		(require-not (literal-prefix "${HOME}/Media/Photos"))))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.securityd"))
		(require-not (global-name "com.apple.mobile.heartbeat"))
		(require-not (global-name "com.apple.FileCoordination"))
		(require-not (global-name "com.apple.trustd"))
		(require-not (global-name "com.apple.xpchelper"))))
(deny network-outbound
	(require-all
		(require-not (control-name "com.apple.flow-divert"))
		(literal "/private/var/run/lockdown/checkin")))
(deny sysctl-read
	(sysctl-name "kern.maxvnodes"))
(deny system-kas-info)
(deny user-preference-read
	(preference-domain "com.apple.afc"))
(deny storage-class-map)
