(version 1)
(allow default)
(deny file-issue-extension
	(require-all
		(extension com.apple.sandbox.container)
		(require-not (extension-class "com.apple.quicklook.readonly"))
		(require-not (extension-class "com.apple.mediaserverd.read-write"))
		(extension-class "com.apple.app-sandbox.read-write")
		(extension-class "com.apple.aned.read-only")
		(extension-class "com.apple.mediaserverd.read")
		(extension-class "com.apple.app-sandbox.read")))
(deny file-read*
	(require-all
		(require-all (require-not (subpath "/AppleInternal/Applications"))
(require-not (subpath "/System/Developer/Applications"))
(require-not (subpath "/private/var/personalized_debug/Applications"))
(require-not (subpath "/private/var/personalized_automation/Applications"))
(require-not (regex #"/private/var/personalized_factory/[UNSUPPORTED]+/Applications/?"))
(require-not (regex #"/private/var/factory_mount/[UNSUPPORTED]+/Applications/?")))
		(require-not (subpath "/private/var/containers/Temp/Bundle"))
		(require-not (subpath "/private/var/containers/Bundle"))
		(require-not (subpath "/Developer/Applications"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Bundle"))
		(require-not (subpath "/private/var/installd"))
		(require-not (subpath "/private/var/personalized_factory"))
		(require-not (subpath "/private/var/tmp"))
		(require-not (subpath "/private/var/db/MobileIdentityData"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Temp/Bundle"))
		(require-not (literal-prefix "${FRONT_USER_HOME}/Library/Preferences/.GlobalPreferences.plist"))
		(require-not (subpath "/Applications"))
		(require-all (require-not (subpath "/private/var/db/os_eligibility"))
(require-not (subpath "/private/var/personalized_quality/Applications")))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}"))
		(extension com.apple.sandbox.container)))
(deny file-read-metadata
	(require-all
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))
		(require-not (extension com.apple.sandbox.container))))
(deny file-write*
	(require-all
		(require-not (subpath "/private/var/containers/Temp/Bundle"))
		(require-not (subpath "/private/var/containers/Bundle"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Bundle"))
		(require-not (subpath "/private/var/installd"))
		(require-not (subpath "/private/var/tmp"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Temp/Bundle"))
		(require-not (subpath-prefix "${PROCESS_TEMP_DIR}"))))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny file-write-xattr
	(require-all
		(require-not (subpath "/private/var/containers/Temp/Bundle"))
		(require-not (subpath "/private/var/containers/Bundle"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Bundle"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Temp/Bundle"))
		(subpath "/private/var/installd")
		(subpath "/private/var/tmp")
		(subpath-prefix "${FRONT_USER_HOME}/Containers/Temp/Bundle")
		(subpath-prefix "${PROCESS_TEMP_DIR}")))
(deny iokit-open-user-client
	(require-not (iokit-user-client-class "AppleMobileFileIntegrityUserClient")))
(deny ipc-posix-sem*
	(ipc-posix-name "installd.firstboot_check"))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.securityd"))
		(require-not (global-name "com.apple.fairplayd.versioned"))
		(require-not (global-name "com.apple.online-auth-agent.xpc"))
		(require-not (global-name "com.apple.lsd.xpc"))
		(require-not (global-name "com.apple.misagent"))
		(require-not (global-name "com.apple.mobile.keybagd.UserManager.xpc"))
		(require-not (global-name "com.apple.nesessionmanager"))
		(require-not (global-name "com.apple.mobile.usermanagerd.xpc"))
		(require-not (global-name "com.apple.iconservices"))
		(require-not (global-name "com.apple.lsd.modifydb"))
		(require-not (global-name "com.apple.securityd.general"))
		(require-all (require-not (global-name "com.apple.lsd.rebuild"))
(require-not (global-name "com.apple.lsd.system.rebuild"))
(require-not (global-name "com.apple.lsd.system.mapdb"))
(require-not (global-name "com.apple.lsd.system.modifydb")))
		(global-name "com.apple.frontboard.systemappservices")
		(extension com.apple.security.exception.mach-lookup.global-name)
		(extension com.apple.security.exception.mach-lookup.local-name)
		(global-name "com.apple.trustd")
		(require-any
			(require-all
				(global-name "com.apple.system.notification_center")
				(entitlement-is-bool-true com.apple.security.on-demand-install-capable)
				(require-any
					(xpc-service-name-regex #".+")
					(xpc-service-name "com.apple.WebKit.")
					(xpc-service-name "com.apple.CoreGraphics.CGPDFService")))
			(require-all
				(global-name "com.apple.lsd.openurl")
				(global-name "com.apple.tccd")
				(global-name "com.apple.lsd.advertisingidentifiers")
				(xpc-service-name-regex #".+")))))
(deny sysctl*
	(sysctl-name "kern.grade_cputype"))
(deny system-kas-info)
(deny user-preference-read
	(preference-domain "com.apple.mobile.installation"))
(deny storage-class-map)
