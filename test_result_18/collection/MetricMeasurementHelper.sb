(version 1)
(allow default)
(deny file-read*
	(require-all (require-not (subpath "/private/var/tmp/com.apple.MetricMeasurementHelper"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.MetricMeasurementHelper"))))
(deny file-write*
	(require-any (subpath "/private/var/tmp/com.apple.MetricMeasurementHelper")
(subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.MetricMeasurementHelper")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny system-kas-info)
(deny storage-class-map)
