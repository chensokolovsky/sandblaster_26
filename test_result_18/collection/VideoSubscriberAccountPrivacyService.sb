(version 1)
(allow default)
(deny file-read*
	(require-all
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Bundle/Application"))
		(require-not (subpath "/private/var/containers/Bundle/Application"))))
(deny file-write-setugid)
(deny job-creation)
(deny system-kas-info)
(deny storage-class-map)
