(version 1)
(allow default)
(deny file-issue-extension
	(require-all
		(require-any (subpath "/private/var/tmp/com.apple.MTLCompilerService")
(subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.MTLCompilerService"))
		(extension-class "com.apple.app-sandbox.read-write")
		(extension-class "com.apple.app-sandbox.read")
		(extension-class "com.apple.mediaserverd.read-write")
		(extension-class "com.apple.aned.read-only")
		(extension-class "com.apple.mediaserverd.read")))
(deny file-read*
	(require-all
		(require-not (subpath "/System/Library/Extensions"))
		(require-all (require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/CrashReporter/DiagnosticLogs/AGX"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/CrashReporter/DiagnosticLogs/MTLCompiler")))
		(require-not (literal-prefix "${FRONT_USER_HOME}/Library/Preferences/.GlobalPreferences_m.plist"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/Metal"))
		(require-all (require-not (subpath "/private/var/tmp/com.apple.MTLCompilerService"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.MTLCompilerService")))
		(require-not (literal "/private/var/Managed Preferences/mobile/.GlobalPreferences_m.plist"))))
(deny file-read-metadata
	(require-all
		(require-all (require-not (subpath "/private/var/tmp/com.apple.MTLCompilerService"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.MTLCompilerService")))
		(require-all (require-not (literal-prefix "${FRONT_USER_HOME}"))
(require-not (literal-prefix "${FRONT_USER_HOME}/Library/Preferences")))
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))))
(deny file-write*
	(require-all
		(require-all (require-not (subpath "/private/var/tmp/com.apple.MTLCompilerService"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.MTLCompilerService")))
		(require-all (require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/CrashReporter/DiagnosticLogs/AGX"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/CrashReporter/DiagnosticLogs/MTLCompiler")))
		(subpath-prefix "${FRONT_USER_HOME}/Library/Logs/Metal")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny ipc-posix-sem*
	(ipc-posix-name "/MDB"))
(deny job-creation)
(deny syscall-unix
	(c1 1d2c 8f73 9ce0))
(deny system-kas-info)
(deny user-preference-read
	(preference-domain "com.apple.Metal"))
(deny storage-class-map)
