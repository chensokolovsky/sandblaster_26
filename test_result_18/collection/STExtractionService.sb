(version 1)
(allow default)
(deny file-map-executable
	(require-not (subpath "/private/var/MobileSoftwareUpdate/MobileAsset/MobileAssetBrain")))
(deny file-read*
	(require-all
		(require-not (subpath "/private/var/MobileSoftwareUpdate/MobileAsset/MobileAssetBrain"))
		(require-all (require-not (subpath "/private/var/tmp/com.apple.STExtractionService"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.STExtractionService")))
		(require-not (extension com.apple.STExtractionService))))
(deny file-read-metadata
	(require-all
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))))
(deny file-write*
	(require-all
		(require-all (require-not (subpath "/private/var/tmp/com.apple.STExtractionService"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.STExtractionService")))
		(extension com.apple.STExtractionService)))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny job-creation)
(deny system-kas-info)
(deny user-preference-read
	(preference-domain "com.apple.STExtractionService"))
(deny storage-class-map)
