		(require-not (preference-domain "com.apple.CFNetwork"))
		(require-not (preference-domain "com.apple.stockholm"))
		(require-not (preference-domain "com.apple.indigo"))
		(require-not (preference-domain "com.apple.mobileactivationd"))
		(preference-domain "com.apple.WebFoundation")))
(deny user-preference-write
	(require-all
		(require-not (preference-domain "com.apple.mobileactivationd"))
		(preference-domain "com.apple.indigo")))
(deny storage-class-map)
