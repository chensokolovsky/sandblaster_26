(version 1)
(allow default)
(deny file-issue-extension
	(require-all
		(extension-class "com.apple.app-sandbox.read-write")
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd"))
	(require-all
		(extension-class "com.apple.app-sandbox.read")
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd"))
	(require-all
		(extension-class "com.apple.mediaserverd.read-write")
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd"))
	(require-all
		(extension-class "com.apple.aned.read-only")
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd"))
	(require-all
		(extension-class "com.apple.mediaserverd.read")
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd"))
	(require-all
		(extension-class "com.apple.nsurlsessiond.readonly")
		(extension com.apple.sandbox.executable)))
(deny file-read*
	(require-all
		(process-attribute is-apple-signed-executable)
		(require-not (literal "/private/var/preferences/com.apple.networkextension.uuidcache.plist")))
	(require-all
		(vnode-type REGULAR-FILE)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.mobileactivationd")))
	(require-all
		(vnode-type DIRECTORY)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.mobileactivationd")))
	(require-all
		(require-not (literal "/private/var/preferences/com.apple.networkd.plist"))
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-all (require-not (subpath "/private/var/tmp/com.apple.mobileactivationd"))
(require-not (subpath "/private/var/logs/mobileactivationd_restore"))
(require-not (subpath "/private/var/hardware/dcrt"))
(require-not (subpath "/private/var/hardware/sdcrt"))
(require-not (subpath "/private/var/hardware/factory/mfi"))
(require-not (subpath "/private/var/hardware/MobileActivation"))
(require-not (literal "/private/var/"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.mobileactivationd"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/mad"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Caches"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Cookies"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/mobileactivationd")))
		(require-not (literal "/private/var/Managed Preferences/mobile/com.apple.SystemConfiguration.plist"))
		(require-not (literal "/private/var/preferences/com.apple.security.plist"))
		(require-all (require-not (subpath "/private/var/hardware/FactoryData"))
(require-not (literal "/usr/standalone/firmware/apticket.der")))
		(require-not (literal "/private/var/db/com.apple.networkextension.tracker-info"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd"))
		(require-not (literal "/private/var/preferences/com.apple.NetworkStatistics.plist"))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(require-not (subpath "/private/preboot"))
		(require-not (literal "/private/var/containers/Shared/SystemGroup/systemgroup.com.apple.nsurlstoragedresources/Library/dafsaData.bin"))
		(entitlement-is-present com.apple.private.networkextension.configuration)
		(require-not (literal "/private/var/preferences/com.apple.networkextension.plist"))))
(deny file-read-metadata
	(require-all
		(require-not (literal-prefix "${HOME}/Library/HTTPStorages"))
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd"))
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Caches"))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))))
(deny file-write*
	(require-all
		(vnode-type REGULAR-FILE)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.mobileactivationd")))
	(require-all
		(vnode-type DIRECTORY)
		(require-not (subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.mobileactivationd")))
	(require-all
		(require-not (subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd"))
		(require-all (require-not (subpath "/private/var/tmp/com.apple.mobileactivationd"))
(require-not (subpath "/private/var/logs/mobileactivationd_restore"))
(require-not (subpath "/private/var/hardware/dcrt"))
(require-not (subpath "/private/var/hardware/sdcrt"))
(require-not (subpath "/private/var/hardware/factory/mfi"))
(require-not (subpath "/private/var/hardware/MobileActivation"))
(require-not (literal "/private/var/"))
(require-not (subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.mobileactivationd"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/mad"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Caches"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Cookies"))
(require-not (subpath-prefix "${FRONT_USER_HOME}/Library/Logs/mobileactivationd")))
		(require-not (subpath-prefix "${HOME}/Library/Cookies"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd")))
(deny file-write-create
	(literal-prefix "${HOME}/Library/Logs/CrashReporter/CFNetwork_")
	(require-all
		(vnode-type DIRECTORY)
		(require-not (literal-prefix "${HOME}/Library/HTTPStorages"))
		(require-not (literal-prefix "${HOME}/Library/Caches"))
		(subpath-prefix "${HOME}/Library/Caches/com.apple.nsurlsessiond/Downloads/com.apple.mobileactivationd")
		(subpath-prefix "${HOME}/Library/HTTPStorages/com.apple.mobileactivationd")
		(require-any (subpath "/private/var/tmp/com.apple.mobileactivationd")
(subpath "/private/var/logs/mobileactivationd_restore")
(subpath "/private/var/hardware/dcrt")
(subpath "/private/var/hardware/sdcrt")
(subpath "/private/var/hardware/factory/mfi")
(subpath "/private/var/hardware/MobileActivation")
(literal "/private/var/")
(subpath-prefix "${PROCESS_TEMP_DIR}/com.apple.mobileactivationd")
(subpath-prefix "${FRONT_USER_HOME}/Library/mad")
(subpath-prefix "${FRONT_USER_HOME}/Library/Caches")
(subpath-prefix "${FRONT_USER_HOME}/Library/Cookies")
(subpath-prefix "${FRONT_USER_HOME}/Library/Logs/mobileactivationd"))
		(subpath-prefix "${HOME}/Library/Cookies")
		(subpath-prefix "${HOME}/Library/Caches/com.apple.mobileactivationd")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "ProvInfoIOKitUserClient"))
		(require-not (iokit-user-client-class "AppleKeyStoreUserClient"))
		(extension com.apple.security.exception.iokit-user-client-class)
		(iokit-user-client-class "RootDomainUserClient")))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.securityd"))
		(require-not (global-name "com.apple.SystemConfiguration.DNSConfiguration"))
		(require-not (global-name "com.apple.networkscored"))
