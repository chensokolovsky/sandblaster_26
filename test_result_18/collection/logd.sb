(version 1)
(allow default)
(deny file-ioctl
	(literal "/dev/oslog"))
(deny file-issue-extension
	(require-all
		(extension-class "com.apple.logd.read-only")
		(require-all (require-not (subpath "/private/var/db/uuidtext"))
(require-not (subpath "/private/var/db/diagnostics")))
		(require-all (require-not (subpath "/private/var/userdata/diagnostics"))
(require-not (subpath "/private/var/db/timesync")))))
(deny file-read*
	(require-all
		(require-all (require-not (subpath "/private/var/db/uuidtext"))
(require-not (subpath "/private/var/db/diagnostics")))
		(require-all (require-not (subpath "/private/var/userdata/diagnostics"))
(require-not (subpath "/private/var/db/timesync")))
		(require-all (require-not (literal "/private/var/log/system."))
(require-not (subpath "/private/var/logs/system_logs.logarchive")))
		(require-not (literal "/dev/oslog"))
		(require-not (subpath "/private/var/preferences/Logging"))))
(deny file-read-xattr
	(require-not (xattr "com.apple.logd.metadata")))
(deny file-write*
	(require-all
		(require-all (require-not (subpath "/private/var/db/uuidtext"))
(require-not (subpath "/private/var/db/diagnostics")))
		(require-not (subpath "/private/var/preferences/Logging"))
		(require-all (require-not (subpath "/private/var/userdata/diagnostics"))
(require-not (subpath "/private/var/db/timesync")))
		(require-any (literal "/private/var/log/system.")
(subpath "/private/var/logs/system_logs.logarchive"))))
(deny file-write-data
	(require-not (literal "/dev/oslog")))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny file-write-xattr
	(require-not (xattr "com.apple.logd.metadata")))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.logd_helper"))
		(require-not (global-name "com.apple.iohideventsystem"))
		(global-name "com.apple.iokit.powerdxpc")
		(global-name "com.apple.PowerManagement.control")
		(extension com.apple.security.exception.mach-lookup.global-name)
		(extension com.apple.security.exception.mach-lookup.local-name)
		(global-name "com.apple.powerlog.plxpclogger.xpc")
		(require-any
			(require-all
				(global-name "com.apple.system.notification_center")
				(entitlement-is-bool-true com.apple.security.on-demand-install-capable)
				(require-any
					(xpc-service-name-regex #".+")
					(xpc-service-name "com.apple.WebKit.")
					(xpc-service-name "com.apple.CoreGraphics.CGPDFService")))
			(require-all
				(global-name "com.apple.lsd.openurl")
				(global-name "com.apple.tccd")
				(global-name "com.apple.lsd.advertisingidentifiers")
				(xpc-service-name-regex #".+")))))
(deny syscall-unix
	(c1 654c 3c95 9ce0))
(deny system-kas-info)
(deny storage-class-map)
