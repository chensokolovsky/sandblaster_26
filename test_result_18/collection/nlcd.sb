(version 1)
(allow default)
(deny file-write-setugid)
(deny job-creation)
(deny mach-lookup
	(require-not (global-name "com.apple.pfd")))
(deny sysctl*
	(sysctl-name "net.link.generic.system.enable_netagent"))
(deny sysctl-read
	(require-all
		(require-not (sysctl-name "net.link.generic.system.if_attach_nx"))
		(require-not (sysctl-name "net.routetable."))))
(deny system-kas-info)
(deny storage-class-map)
