(version 1)
(allow default)
(deny file-ioctl
	(require-any (literal "/dev/nfc.log")
(literal "/dev/spmi.nfc")
(literal "/dev/tty.stockholm")))
(deny file-read*
	(require-all
		(require-not (literal "/private/var/preferences/com.apple.networkd.plist"))
		(require-not (literal "/private/var/preferences/SystemConfiguration/com.apple.radios.plist"))
		(require-all (require-not (subpath-prefix "${HOME}/Library/NFStorage"))
(require-not (literal-prefix "${HOME}/Library/Logs/CrashReporter/DiagnosticLogs/NearField")))
		(require-not (literal "/private/var/MobileSoftwareUpdate/lastOTA/ota_tolerated_failures.plist"))
		(require-not (subpath "/private/var/containers/Bundle"))
		(require-all (require-not (literal "/dev/nfc.log"))
(require-not (literal "/dev/spmi.nfc"))
(require-not (literal "/dev/tty.stockholm")))
		(require-all (require-not (subpath "/AppleInternal/Applications"))
(require-not (subpath "/System/Developer/Applications"))
(require-not (subpath "/private/var/personalized_debug/Applications"))
(require-not (subpath "/private/var/personalized_automation/Applications"))
(require-not (regex #"/private/var/personalized_factory/[UNSUPPORTED]+/Applications/?"))
(require-not (regex #"/private/var/factory_mount/[UNSUPPORTED]+/Applications/?")))
		(require-not (subpath "/Developer/Applications"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Bundle"))
		(require-all (require-not (literal-prefix "${HOME}/Library/Logs/nfcd"))
(require-not (literal-prefix "${HOME}/Library/Logs/awd/awd-nfcd.log")))
		(require-not (literal "/private/var/db/os_eligibility/eligibility.plist"))
		(require-not (subpath "/Applications"))
		(require-not (subpath "/private/preboot"))
		(require-not (subpath "/private/var/tmp"))
		(require-all (require-not (subpath "/private/var/hardware/FactoryData"))
(require-not (literal "/usr/standalone/firmware/apticket.der")))
		(require-any
			(require-all
				(extension com.apple.assets.read)
				(require-not (subpath "/private/var/MobileAsset"))
				(require-not (subpath-prefix "${HOME}/Library/Assets")))
			(require-all
				(require-not (subpath "/usr/standalone/firmware/SLAM"))
				(require-not (subpath-prefix "${PROCESS_TEMP_DIR}"))
				(require-not (literal-prefix "${HOME}/Library/Logs/AppleSSE.log"))))))
(deny file-read-metadata
	(require-all
		(require-not (literal-prefix "${HOME}/Library/Logs/CrashReporter/DiagnosticLogs"))
		(require-not (literal-prefix "${HOME}/Library/Logs/CrashReporter"))
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))
		(require-not (literal-prefix "${HOME}/Library/Logs/awd"))))
(deny file-write*
	(require-all
		(require-all (require-not (subpath-prefix "${HOME}/Library/NFStorage"))
(require-not (literal-prefix "${HOME}/Library/Logs/CrashReporter/DiagnosticLogs/NearField")))
		(require-all (require-not (literal-prefix "${HOME}/Library/Logs/nfcd"))
(require-not (literal-prefix "${HOME}/Library/Logs/awd/awd-nfcd.log")))
		(require-not (subpath "/private/var/tmp"))
		(require-not (literal-prefix "${HOME}/Library/Logs/AppleSSE.log"))
		(subpath-prefix "${PROCESS_TEMP_DIR}")))
(deny file-write-create
	(require-all
		(vnode-type DIRECTORY)
		(require-not (literal-prefix "${HOME}/Library/Logs/CrashReporter/DiagnosticLogs"))
		(require-not (literal-prefix "${HOME}/Library/Logs/CrashReporter"))
		(require-not (literal-prefix "${HOME}/Library/Logs/awd"))))
(deny file-write-data
	(require-all (require-not (literal "/dev/nfc.log"))
(require-not (literal "/dev/spmi.nfc"))
(require-not (literal "/dev/tty.stockholm"))))
(deny file-write-setugid)
(deny file-write-unlink
	(entitlement-is-present com.apple.private.oop-jit.loader)
	(require-all
		(extension com.apple.sandbox.oopjit)
		(require-not (subpath "/private/var/OOPJit"))))
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "AppleCredentialManagerUserClient"))
		(require-not (iokit-user-client-class "AppleSMCClient"))
		(require-not (iokit-user-client-class "AppleStockholmControlUserClient"))
		(require-not (iokit-user-client-class "AppleSSEUserClient"))
		(require-not (iokit-user-client-class "ApplePPMUserClient"))))
(deny iokit-set-properties
	(require-all
		(require-any (iokit-property "IOPMUBootLPMCtrl")
(iokit-property "IOPMUBootLPMState"))
		(iokit-user-client-class "AppleDialogSPMIPMU")))
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.securityd"))
		(require-not (global-name "com.apple.powerlog.plxpclogger.xpc"))
		(require-not (global-name "com.apple.SystemConfiguration.helper"))
		(require-all (require-not (global-name "com.apple.nfrestore"))
(require-not (global-name "com.apple.nfcacd.recovery.request")))
		(require-not (global-name "com.apple.commcenter.xpc"))
		(require-not (global-name "com.apple.SystemConfiguration.configd"))
		(require-not (global-name "com.apple.coreduetd"))
		(require-not (global-name "com.apple.SharingServices"))
		(require-not (global-name "com.apple.nfcacd.hwmanager"))
		(require-not (global-name "com.apple.SBUserNotification"))
		(require-not (global-name "com.apple.accessibility.AXBackBoardServer"))
		(require-not (global-name "com.apple.trustd"))
		(require-not (global-name "com.apple.accessibility.AXSpringBoardServer"))
		(require-not (global-name "com.apple.mobileactivationd"))
		(require-not (global-name "com.apple.BarcodeSupport.BarcodeNotificationService"))
		(require-not (global-name "com.apple.awdd"))
		(require-not (global-name "com.apple.iokit.powerdxpc"))
		(require-not (global-name "com.apple.iohideventsystem"))
		(require-not (global-name "com.apple.mobile.softwareupdated"))
		(require-not (global-name "com.apple.PowerManagement.control"))
		(global-name "com.apple.frontboard.systemappservices")))
(deny network-outbound
	(require-all
		(require-all (require-not (control-name "com.apple.spmi.nfc"))
(require-not (control-name "com.apple.uart.stockholm")))
		(control-name "com.apple.flow-divert")))
(deny system-kas-info)
(deny system-socket
	(socket-domain AF_SYSTEM))
(deny user-preference-read
	(require-all
		(require-not (preference-domain "com.apple.seserviced.contactlessCredential.settings"))
		(require-all (require-not (preference-domain "nfcd"))
(require-not (preference-domain "com.apple.nfcd.nfttr"))
(require-not (preference-domain "com.apple.stockholm.analytics"))
(require-not (preference-domain "com.apple.stockholm.fail-forward"))
(require-not (preference-domain "com.apple.stockholm.wallet.presentation")))
		(require-not (preference-domain "com.apple.nfcd"))
		(require-not (preference-domain "com.apple.AppSupport"))
		(require-not (preference-domain "com.apple.stockholm.atl"))
		(require-not (preference-domain "com.apple.purplebuddy"))
		(require-not (preference-domain "com.apple.passd.class-d"))
		(require-not (preference-domain "com.apple.stockholm.awd"))
		(require-not (preference-domain "com.apple.stockholm"))
		(require-not (preference-domain "com.apple.Duet"))
		(require-not (preference-domain "com.apple.homed"))
		(preference-domain "com.apple.MobileAsset")))
(deny user-preference-write
	(require-all
		(require-not (preference-domain "com.apple.stockholm.atl"))
		(require-all (require-not (preference-domain "nfcd"))
(require-not (preference-domain "com.apple.nfcd.nfttr"))
(require-not (preference-domain "com.apple.stockholm.analytics"))
(require-not (preference-domain "com.apple.stockholm.fail-forward"))
(require-not (preference-domain "com.apple.stockholm.wallet.presentation")))
		(require-not (preference-domain "com.apple.stockholm.awd"))
		(require-not (preference-domain "com.apple.stockholm"))
		(preference-domain "com.apple.nfcd")))
(deny storage-class-map)
