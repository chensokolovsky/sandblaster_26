(version 1)
(allow default)
(deny file-read*
	(require-all
		(require-not (literal "/usr/sbin"))
		(require-all (require-not (literal "/usr"))
(require-not (literal "/usr/sbin/wirelessproxd")))))
(deny file-read-metadata
	(require-all
		(require-not (literal-prefix "${HOME}"))
		(require-not (literal-prefix "${HOME}/Library/Preferences"))))
(deny file-write-setugid)
(deny job-creation)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.server.bluetooth.le.pipe.xpc"))
		(require-not (global-name "com.apple.server.bluetooth.le.att.xpc"))
		(require-not (global-name "com.apple.touchsetupd.wirelessproximity"))
		(require-not (global-name "com.apple.awdd"))
		(require-not (global-name "com.apple.distributed_notifications@1v3"))))
(deny system-kas-info)
(deny user-preference-read
	(preference-domain "com.apple.wirelessproxd.debug"))
(deny storage-class-map)
