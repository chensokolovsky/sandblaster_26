(version 1)
(allow default)
(deny file-read*
	(require-all
		(vnode-type REGULAR-FILE)
		(require-not (file-mode #o0001))
		(require-not (file-mode #o0100))
		(require-not (file-mode #o0010)))
	(require-all
		(require-all (require-not (subpath "/private/var/db/uuidtext"))
(require-not (subpath "/private/var/db/diagnostics")))
		(require-all (require-not (subpath "/AppleInternal/Applications"))
(require-not (subpath "/System/Developer/Applications"))
(require-not (subpath "/private/var/personalized_debug/Applications"))
(require-not (subpath "/private/var/personalized_automation/Applications"))
(require-not (regex #"/private/var/personalized_factory/[UNSUPPORTED]+/Applications/?"))
(require-not (regex #"/private/var/factory_mount/[UNSUPPORTED]+/Applications/?")))
		(require-not (subpath "/private/var/containers/Bundle"))
		(require-all (require-not (subpath "/private/var/userdata/diagnostics"))
(require-not (subpath "/private/var/db/timesync")))
		(require-not (subpath "/Developer/Applications"))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/Containers/Bundle"))
		(require-not (subpath "/private/var/tmp/FTABHarvest"))
		(require-all (require-not (literal "/private/var/log/system."))
(require-not (subpath "/private/var/logs/system_logs.logarchive")))
		(require-not (subpath-prefix "${FRONT_USER_HOME}/tmp/FTABHarvest"))
		(require-not (subpath "/Applications"))
		(subpath "/private/var/preferences/Logging")))
(deny file-read-metadata
	(require-not (literal "/private/var/userdata")))
(deny file-write-create
	(require-all
		(vnode-type DIRECTORY)
		(require-not (literal "/private/var/userdata"))))
(deny file-write-setugid)
(deny iokit-open-user-client
	(require-not (iokit-user-client-class "RTBuddyUserClient")))
(deny job-creation)
(deny sysctl-read
	(sysctl-name "kern.filesetuuid"))
(deny system-kas-info)
(deny storage-class-map)
