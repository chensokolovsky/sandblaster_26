(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-set-properties)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.securityd"))
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.runningboard"))
		(require-not (global-name "com.apple.pluginkit.pkd"))
		(require-not (global-name "com.apple.CoreAuthentication.daemon.libxpc"))
		(require-not (global-name "com.apple.system.libinfo.muser"))
		(require-not (global-name "com.apple.diagd"))
		(require-not (global-name "com.apple.ctkd.token-client"))
		(require-not (global-name "com.apple.CoreAuthentication.daemon"))
		(require-not (global-name "com.apple.cfprefsd.daemon"))
		(require-not (global-name "com.apple.lsd.mapdb"))
		(require-not (global-name "com.apple.ctkd.slot-client"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.cfprefsd.daemon.system"))
		(require-all (require-not (xpc-service-name "com.apple.ist.AppleConnect.CTKExtension"))
(require-not (xpc-service-name "com.apple.ist.AppleConnect.CTKExtension.apple-extension-service"))
(require-not (xpc-service-name "com.apple.CryptoTokenKit.pivtoken"))
(require-not (xpc-service-name "com.apple.CryptoTokenKit.pivtoken.apple-extension-service"))
(require-not (xpc-service-name "com.apple.CryptoTokenKit.secelemtoken"))
(require-not (xpc-service-name "com.apple.CryptoTokenKit.secelemtoken.apple-extension-service")))
		(require-not (xpc-service-name-regex #".+"))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny system-kas-info)
(deny storage-class-map)
