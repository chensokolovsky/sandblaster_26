(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "AppleSMCClient"))
		(require-not (iokit-user-client-class "AppleMobileApNonceUserClient"))))
(deny iokit-open-service
	(require-all
		(require-not (iokit-user-client-class "AppleSMC"))
		(require-not (iokit-user-client-class "AppleMobileApNonce"))))
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-issue-extension)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.mobileasset.autoasset"))
		(require-not (global-name "com.apple.logd.events"))
		(require-not (global-name "com.apple.seeding.client"))
		(require-not (global-name "com.apple.commcenter.xpc"))
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.mobileassetd.v2"))
		(require-not (global-name "com.apple.system.libinfo.muser"))
		(require-not (global-name "com.apple.system.logger"))
		(require-not (global-name "com.apple.diagd"))
		(require-not (global-name "com.apple.lsd.mapdb"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.containermanagerd"))
		(require-not (xpc-service-name "com.apple.ImageIOXPCService"))
		(global-name "com.apple.cfprefsd.daemon")
		(global-name "com.apple.cfprefsd.daemon.system")
		(global-name "com.apple.analyticsd")
		(global-name "com.apple.FileCoordination")))
(deny process-exec*
	(require-all
		(literal "/usr/local/bin/configdarwinbootargs")
		(debug-mode))
	(require-all
		(require-all (require-not (literal "/System/Library/Filesystems/apfs.fs/newfs_apfs"))
(require-not (literal "/System/Library/Filesystems/apfs.fs/mount_apfs"))
(require-not (literal "/System/Library/Filesystems/hfs.fs/mount_hfs"))
(require-not (literal "/System/Library/Filesystems/hfs.fs/Contents/Resources/newfs_hfs")))
		(require-not (literal "/sbin/mount"))))
(deny socket-ioctl)
(deny syscall-unix
	(require-not (c1 2773 6adc 4c20)))
(deny syscall-mach
	(require-not (cb 277d 6adc 4c1e)))
(deny syscall-mig
	(require-not (cc 277f 6adc 4c1c)))
(deny sysctl-write
	(require-not (sysctl-name "vfs.generic.apfs.proc_free_blocks_threshold")))
(deny system-fcntl
	(require-not (ce 278c 6adc 4c19)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
