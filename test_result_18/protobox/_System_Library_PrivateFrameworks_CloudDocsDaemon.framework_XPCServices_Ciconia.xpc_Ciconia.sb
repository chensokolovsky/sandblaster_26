(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-set-properties)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.logd"))))
(deny process-exec*
	(require-all
		(literal "/usr/local/bin/dastool")
		(debug-mode))
	(require-all
		(require-not (literal "/usr/bin/awk"))
		(require-all (require-not (literal "/usr/bin/env"))
(require-not (literal "/usr/bin/fileproviderctl")))))
(deny system-fsctl
	(require-all
		(fsctl-command-regex [UNSUPPORTED](_IO "J" 2))
		(fsctl-command-regex [UNSUPPORTED](_IO "J" 32))))
(deny system-kas-info)
(deny storage-class-map)
