		(require-not (global-name "com.apple.FileCoordination"))
		(global-name "com.apple.AppSSO.service-xpc")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl
	(require-not (ioctl-command-regex [UNSUPPORTED](_IO "s" 152))))
(deny syscall-unix
	(require-not (c1 0f07 6adc 6374)))
(deny syscall-mach
	(require-not (cb 0f11 6adc 6372)))
(deny syscall-mig
	(require-not (cc 0f13 6adc 6370)))
(deny system-fcntl
	(require-not (ce 09df 6adc 636e)))
(deny system-kas-info)
(deny system-necp-client-action
	(require-all
		(require-not (d3 0f19 6a77 6add))
		(d3 019d 6a78 6add)
		(necp-client-action-regex [UNSUPPORTED])
		(necp-client-action-regex [UNSUPPORTED])))
(deny storage-class-map)
