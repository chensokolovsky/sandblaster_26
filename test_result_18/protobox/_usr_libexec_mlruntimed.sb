(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-set-properties)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.networkscored"))
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.trial.status"))
		(require-not (global-name "com.apple.logd.events"))
		(require-not (global-name "com.apple.runningboard"))
		(require-not (global-name "com.apple.pluginkit.pkd"))
		(require-not (global-name "com.apple.system.libinfo.muser"))
		(require-not (global-name "com.apple.diagd"))
		(require-not (global-name "com.apple.parsecd"))
		(require-not (global-name "com.apple.photos.service"))
		(require-not (global-name "com.apple.mobilegestalt.xpc"))
		(require-not (global-name "com.apple.lsd.mapdb"))
		(require-not (global-name "com.apple.distributed_notifications@1v3"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.photoanalysisd"))
		(require-all (require-not (xpc-service-name "com.apple.MediaMLPluginApp.MediaMLPlugin"))
(require-not (xpc-service-name "com.apple.aiml.mlpt.PriML.FedStats.Plugin"))
(require-not (xpc-service-name "com.apple.aiml.mlpt.PriML.FedStats.PluginClassB"))
(require-not (xpc-service-name "com.apple.biome.BiomeStreams.BiomeLighthousePlugin"))
(require-not (xpc-service-name "com.apple.PhotoAnalysis.PhotoAnalysisLighthousePlugin"))
(require-not (xpc-service-name "com.apple.AppleMediaDiscovery.AppStoreEvalLighthousePlugin"))
(require-not (xpc-service-name "com.apple.siri.SiriInvocationAnalytics.SiriInvocationAnalyticsPlugin"))
(require-not (xpc-service-name "com.apple.dp.PrivateFederatedLearning.DPMLRuntimePlugin"))
(require-not (xpc-service-name "com.apple.dp.PrivateFederatedLearning.DPMLRuntimePluginClassB"))
(require-not (xpc-service-name "com.apple.dp.PrivateFederatedLearning.DPMLRuntimePluginNonDnU")))
		(require-any
			(require-all
				(xpc-service-name-regex #".+")
				(require-not (extension com.apple.pluginkit.plugin-service)))
			(require-all
				(global-name "com.apple.containermanagerd.system")
				(global-name "com.apple.cfprefsd.daemon")
				(global-name "com.apple.cfprefsd.daemon.system")
				(global-name "com.apple.analyticsd")
				(global-name "com.apple.FSEvents")))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny system-kas-info)
(deny storage-class-map)
