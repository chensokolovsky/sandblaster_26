(deny process-exec*)
(deny process-exec-interpreter)
(deny syscall-unix
	(require-not (c1 6334 6adc 084a)))
(deny syscall-mach
	(require-not (cb 633e 6adc 0848)))
(deny syscall-mig
	(require-not (cc 6340 6adc 0846)))
(deny system-fcntl
	(require-not (ce 4966 6adc 0844)))
(deny system-kas-info)
(deny storage-class-map)
