(version 1)
(allow default)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-user-client
	(require-all
		(require-not (iokit-user-client-class "IOSurfaceRootUserClient"))
		(require-not (iokit-user-client-class "IOSurfaceAcceleratorParavirtClient"))
		(require-not (iokit-user-client-class "IOGPUDeviceUserClient"))
		(iokit-user-client-class "AppleVideoToolboxParavirtualizationUserClient")))
(deny iokit-set-properties)
(deny job-creation)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.diagnosticd"))
		(require-not (global-name "com.apple.logd.events"))
		(require-not (global-name "com.apple.runningboard"))
		(require-not (global-name "com.apple.bird"))
		(require-not (global-name "com.apple.mobile.keybagd.UserManager.xpc"))
		(require-not (global-name "com.apple.gpumemd.source"))
		(require-not (global-name "com.apple.system.libinfo.muser"))
		(require-not (global-name "com.apple.diagd"))
		(require-not (global-name "com.apple.mobile.usermanagerd.xpc"))
		(require-not (global-name "com.apple.analyticsd"))
		(require-not (global-name "com.apple.mobilegestalt.xpc"))
		(require-not (global-name "com.apple.cfprefsd.daemon"))
		(require-not (global-name "com.apple.lsd.mapdb"))
		(require-not (global-name "com.apple.distributed_notifications@1v3"))
		(require-not (global-name "com.apple.logd"))
		(require-not (global-name "com.apple.containermanagerd"))
		(require-not (global-name "com.apple.containermanagerd.system"))
		(require-not (global-name "com.apple.cfprefsd.daemon.system"))
		(require-all (require-not (xpc-service-name "com.apple.iBooks.BooksThumbnail"))
(require-not (xpc-service-name "com.apple.clips.quicklookThumbnailing"))
(require-not (xpc-service-name "com.apple.shortcuts.ThumbnailExtension"))
(require-not (xpc-service-name "com.apple.AssetViewer.ASVAssetThumbnail"))
(require-not (xpc-service-name "com.apple.Bridge.GreenfieldThumbnailExtension"))
(require-not (xpc-service-name "com.apple.CoreSVG.thumbnail"))
(require-not (xpc-service-name "com.apple.CoreGraphics.CGPDFService"))
(require-not (xpc-service-name "com.apple.mobilelogic.thumbnailext"))
(require-not (xpc-service-name "com.apple.mobilegarageband.thumbnailext"))
(require-not (xpc-service-name "com.apple.PDFKit.OFD-Thumbnail"))
(require-not (xpc-service-name "com.apple.PassKit.PassKitThumbnailExtension"))
(require-not (xpc-service-name "com.apple.Playgrounds.PlaygroundsThumbnailExtension"))
(require-not (xpc-service-name "com.apple.Mica-Player.PlayerQuickLookThumbnail"))
(require-not (xpc-service-name "com.apple.MQLTester.QLTesterThumbnailExtension"))
(require-not (xpc-service-name "com.apple.UserNotificationsUIKit.ThumbnailProvider"))
(require-not (xpc-service-name "com.apple.UserNotificationsServer.UserNotificationsThumbnailProvider"))
(require-not (xpc-service-name "com.apple.quicklook.TestHost.TestHostThumbnailExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.WebExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.TextExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.FontExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.iWorkExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.ImageExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.OfficeExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.AudiovisualExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.ContactExtension"))
(require-not (xpc-service-name "com.apple.quicklook.thumbnail.CalendarExtension")))
		(require-not (xpc-service-name-regex #".+"))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny system-kas-info)
(deny storage-class-map)
