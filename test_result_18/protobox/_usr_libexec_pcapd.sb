(version 1)
(allow default)
(deny file-ioctl
	(require-all
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "B" 109)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "B" 125)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "B" 120)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "B" 108)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "B" 127)))
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "h" 4)))
		(ioctl-command-regex [UNSUPPORTED](_IO "B" 102))))
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-issue-extension)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (global-name "com.apple.system.notification_center"))
		(require-not (global-name "com.apple.trustd"))
		(global-name "com.apple.diagnosticd")
		(global-name "com.apple.logd")
		(global-name "com.apple.cfprefsd.daemon")
		(global-name "com.apple.cfprefsd.daemon.system")
		(global-name "com.apple.aggregated")))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl
	(require-all
		(require-not (ioctl-command-regex [UNSUPPORTED](_IO "i" 143)))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 144))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 120))
		(ioctl-command-regex [UNSUPPORTED](_IO "i" 121))))
(deny syscall-unix
	(require-all
		(require-not (c1 5299 66eb 1cb5))
		(require-any
			(syscall-number SYS_sigaltstack)
			(require-all
				(syscall-number SYS_openat)
				(syscall-number SYS_fstatat)
				(syscall-number SYS_open)
				(syscall-number 38)
				(syscall-number SYS_memorystatus_control)
				(syscall-number SYS_fstatat64)
				(syscall-number SYS_dup)
				(syscall-number SYS_getfsstat64)
				(syscall-number SYS_map_with_linking_np)
				(syscall-number SYS_getfsstat)))))
(deny syscall-mach
	(require-not (cb 2afe 6adc 1cb3)))
(deny syscall-mig
	(require-not (cc 52a3 6adc 1cb1)))
(deny system-fcntl
	(require-not (ce 0739 6adc 1caf)))
(deny system-kas-info)
(deny system-necp-client-action)
(deny storage-class-map)
